    agent any 
    stages {
        stage('Build') { 
            steps {
                // run script to pull users
                sh 'pwd'
                // convert mardown file to pdf
            }
        }
        stage('Test') { 
            steps {
                // Perform tests eg: does file exist?
                sh 'ls -la'
            }
        }
        stage('Deploy') { 
            steps {
                // Start server
                sh 'echo test'
            }
        }
    }
}